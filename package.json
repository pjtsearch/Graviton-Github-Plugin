{
  "name": "github",
  "id": "github-plugin",
  "version": "0.11.2",
  "description": "Github plugin for Graviton",
  "mainDev": "dist/index.js",
  "main": "index.js",
  "author": "PJTSearch",
  "license": "GPL-3.0-only",
  "private": false,
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@octokit/rest": "^17.1.4",
    "dompurify": "^2.0.11",
    "haunted": "^4.7.0",
    "htm": "^3.0.4",
    "isomorphic-git": "^1.3.1",
    "marked": "^1.0.0",
    "mdi-preact": "^7.1.0",
    "parse-github-url": "^1.0.2",
    "preact": "^10.4.1",
    "preact-css-styled": "^0.1.3",
    "tslib": "^1.11.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-syntax-async-generators": "^7.8.4",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@rollup/plugin-sucrase": "^3.0.0",
    "@rollup/plugin-typescript": "^4.0.0",
    "@types/dompurify": "^2.0.1",
    "@types/marked": "^0.7.4",
    "@types/parse-github-url": "^1.0.0",
    "husky": "^4.2.5",
    "prettier": "2.0.5",
    "rollup": "^2.4.0",
    "rollup-plugin-copy": "^3.3.0",
    "typescript": "^3.8.3"
  },
  "scripts": {
    "build": "rollup --config ./rollup.config.js",
    "watch": "rollup --config ./rollup.config.js --watch",
    "release": "rollup --config ./rollup.config.release.js",
    "types": "tsc --noEmit",
    "format": "prettier --write --no-semi --print-width 120 \"src/**/*.js\";prettier --write --no-semi --print-width 120 \"src/**/*.ts\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn format"
    }
  },
  "alias": {
    "react": "preact/compat",
    "react-dom": "preact/compat"
  }
}
